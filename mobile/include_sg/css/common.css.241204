* {
	text-size-adjust: none !important;
	-webkit-text-size-adjust: none !important;
	font-family:'source-sans-pro','Montserrat','NotoSansKR',sans-serif;
	box-sizing: border-box;
	word-break: break-word;
	overflow-scrolling: touch;
	-webkit-overflow-scrolling: touch;
	  -ms-user-select: none; -moz-user-select: -moz-none; -webkit-user-select: none; -khtml-user-select: none; user-select:none;
}

html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
	margin:0;
	padding:0;
	border:0;
	outline:0;
	font-size:100%;
	vertical-align:baseline;
	background:transparent;
}

body {
	line-height:1;

}

article,aside,details,figcaption,figure,
footer,header,hgroup,menu,nav,section {
	display:block;
}

nav ul {
	list-style:none;
}

blockquote, q {
	quotes:none;
}


ul li {
	list-style: none;
}


.fl {
  float: left;
}

.fr {
  float: right
}

.cf::after {
  content: "";
  display: block;
  clear: both;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content:'';
	content:none;
}

a {
	margin:0;
	padding:0;
	font-size:100%;
	vertical-align:baseline;
	background:transparent;
    text-decoration: none;
}

/* change colours to suit your needs */
ins {
	background-color:#ff9;
	color:#000;
	text-decoration:none;
}

/* change colours to suit your needs */
mark {
	background-color:#ff9;
	color:#000;
	font-style:italic;
	font-weight:bold;
}

del {
	text-decoration: line-through;
}

abbr[title], dfn[title] {
	border-bottom:1px dotted;
	cursor:help;
}

table {
	border-collapse:collapse;
	border-spacing:0;
}

/* change border colour to suit your needs */
hr {
	display:block;
	height:1px;
	border:0;
	border-top:1px solid #cccccc;
	margin:1em 0;
	padding:0;
}

input, select {
	vertical-align:middle;
}

html, body {
	width:640px;
	height:100%;
	margin: 0 auto;
	position: relative;

}

body {
	background:#fff;
}

body > div {
	position:relative;
}

header {
	position: absolute;
	z-index: 15;
	width: 640px;
	overflow: hidden;
}


header.active .gnb_btn {
	border: 1px solid #8092a6;
}
header .gnb_btn {
	display: block;
	position: absolute;
	right: 40px;
	top: 40px;
	width: 44px;
	height:44px;
	transition: all 0.3s ease-out;
	background-color: transparent;
	border: 0 none;
	outline: 0 none;
}
header .gnb_btn .line {
	display: block;
	width: 32px;
	height: 1px;
	background-color: #fff;
	position: absolute;
	transition: all 0.3s ease-out;
}
header.active .gnb_btn .line.first {
    transform: rotate(-45deg);
    top: 22px;
    left:5px;
}
header.active .gnb_btn .line.second {
    transform: rotate(45deg);
    top: 22px;
    left:5px;
}
header.active .gnb_btn .line.second,
header.active .gnb_btn .line.first {
	background-color: #8092a6;
}
header .gnb_btn .line.first {
	top: 10px;
	left: 13px;
}
header .gnb_btn .line.second {
	top: 28px;
	left: 13px;
}

header.active{
	z-index: 52
}

header ul {
  margin: 0px;
  padding: 0px;
  vertical-align: top;
  display: block;
}

header ul li {
  text-align: right;
}

header ul.gnb {
  margin-top: 0px;
  padding-bottom: 85px;
}

header ul.gnb>li>ul {
  display: none;
}

header ul.gnb>li {
  transition: none;
}

header ul li a {
  display: block;
	font-family:'source-sans-pro','Montserrat','NotoSansKR',sans-serif;
  color: rgba(255, 255, 255, 1);
  text-align: right;
  font-weight: 300;
  font-size: 40px;
  line-height: 40px;
  transition: color .3s ease-out;
}

header .util:after {
	content: "";
	display: table;
	clear: both;
}

header .util li {
	float: left;
	display: block;
}

header .util li:nth-of-type(2) {
  padding-right: 40px;
}

header .util li:last-child {
  padding-right: 0px;
}

header .util li a {
  color: rgba(255, 255, 255, 0.5);
  font-size: 30px;
  font-weight: 300;
  text-transform: uppercase;
}

header  .util_top {
  position:absolute;
  top:36px;
  right:124px;
  text-align:left;
}

header .util_top li{
  display:inline-block;
  padding:0px 8px;
}

header .util_top li a{
  font-size:16px;
  font-weight:300;
  line-height:24px;
  text-transform:uppercase;
 }

header .nav-menu-button-svg {
  margin-top: -5px;
  margin-left: -1228px;
  stroke-linecap: round;
}

header .nav-menu-button-line {
  fill: none;
  stroke: rgba(255, 255, 255, .9);
}

header .nav-menu-button-line.button-line-main {
  stroke-width: 2px
}

header .nav-menu-button-line.button-line-shadow {
  opacity: 0.5;
}

header {
  position: absolute;
	top: 0;
	left: 0;
	width: 640px;
  height: 115px;
  background: rgba(255, 255, 255, 0);
  z-index: 50;
}

header nav {
    position: absolute;
    padding: 0px;
    top: 115px;
    box-sizing: border-box;
    padding: 0px;
    width: 100%;
    min-height: calc(100% - 190px);
    transition: opacity 0.6s ease-out;
    background: #5f6c7d;
}

header.active nav::after {
  visibility: visible;
}
header nav ul.gnb>li>ul {
  display: block;
}
header nav ul.gnb {
  position: relative;
  overflow-y: auto;

}
header nav ul.gnb>li {
  overflow: hidden;
  position: relative;
  height: 110px;
  padding: 0px 110px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  transition: all .3s ease-out;
  background:rgba(0,0,0,0);
}
header nav>ul.gnb>li>a::after {
  content: "";
  display: inline-block;
  position: relative;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  position: absolute;
  margin-top: 36px;
  margin-left: 3px;
  background-color: #ffffff;
  opacity: 0;
  transition: opacity .3s ease-out;
}
header nav ul li {
  display: block;
}

header .util {
	position: fixed;
	bottom: 0;
	padding: 0 50px;
	text-align: left;
	background-color: #222;
	width: 640px;
	box-sizing: border-box;
	display: none;
	z-index: 53;
}
header .util > li > a {
	color: rgba(255, 255, 255, 0.3);
	padding-right: 35px;
	display: block;
	line-height: 82px;
}
header .util > li.active > a {
    color: rgba(255, 255, 255, 1);
    display: block;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
}
header nav ul.gnb li ul.depth3 {
  padding-left: 25px;
}
header nav ul.gnb li ul {
  padding-left: 35px;
  margin-bottom: 20px;
  position: relative;
}
header nav ul.gnb li ul.depth3:after {
	width: 1px;
	left: 1px;
}
header nav ul.gnb li ul:after {
  content: "";
  position: absolute;
  left: 3px;
  top: 0px;
  height: 0%;
  width: 2px;
  background-color: #ffffff;
  opacity: 0;
  transition: height .3s ease-out .2s;
}
header nav ul.gnb>li.slide-down>a::after {
  opacity: 1;
}
header nav ul.gnb>li.slide-down>a {
  color: #ffffff;
}
header nav ul.gnb>li.slide-down ul::after {
  height: 100%;
  opacity: 1;
}
header nav ul.gnb>li.slide-down:nth-of-type(1) {
  height: 250px;
}
header nav ul.gnb>li.slide-down:nth-of-type(2) {
  height: 585px;
}

header nav ul.gnb>li.slide-down:nth-of-type(3) {
  height: 530px;
}
header nav ul.gnb>li.slide-down:nth-of-type(4) {
  height: 110px;

}
header nav ul.gnb>li.slide-down:nth-of-type(5) {
  height: 110px;
}
header nav ul.gnb > a {
	line-height: 40px;
	font-size: 20px;
}

header nav ul.depth2  a {
	line-height: 56px;
	font-size: 32px;
}
header nav ul.depth3  a {
	line-height: 40px;
	font-size: 24px;
}
header nav ul.depth4  a {
	line-height: 56px;
	font-size: 20px;
}
header nav ul.gnb li ul.depth4:after{
	width:0px;
}
header nav ul.gnb li ul.depth4 li{
	position: relative;
}
header nav ul.gnb li ul.depth4 li:after{
	position: absolute;
	content:"";
	background:#d10409;
	border-radius: 50%;
	width:5px;
	top:25px;
	left:-20px;
	height:5px;
}
header nav ul li a {
  line-height: 50px;
  color: #fff;
  font-weight: 300;
	text-transform: uppercase;
  text-align: left;
}
header nav ul.depth2 li.active a{
	color: #ff6a2f
}
header.active nav {
  opacity: 1;
}
